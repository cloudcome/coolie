    "type": "git",
    "url": "https://github.com/cooliejs/coolie.js"
  },
  "bugs": {
    "url": "https://github.com/cooliejs/coolie.js/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "jasmine-core": "^2.2.0",
    "karma": "^0.13.22",
    "karma-coverage": "^0.3.1",
    "karma-coveralls": "^0.1.5",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^0.1.4",
    "uglify-js": "^2.4.15"
  },
  "dependencies": {},
